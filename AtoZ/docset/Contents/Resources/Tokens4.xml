		<Token>
			<TokenIdentifier>//apple_ref/occ/instm/AZBoxGrid/mouseDragged:</TokenIdentifier>
			<Abstract>	AZBox *i = [self cellAtIndex:index];
	NSSize zDragOffset = NSMakeSize(0.0, 0.0);
    NSPasteboard *zPasteBoard = [NSPasteboard pasteboardWithName:NSDragPboard];
    [zPasteBoard declareTypes:[NSArray arrayWithObject:NSTIFFPboardType]
                        owner:self];
	NSImage snap = [(NSView)i snapshot];
    [zPasteBoard setData:[snap TIFFRepresentation] 
                 forType:NSTIFFPboardType];
	
	NSPoint originFlip  = i.frame.origin;
	originFlip.y += i.frame.size.height;
    [self dragImage:snap
                 at:originFlip
             offset:zDragOffset
              event:event
         pasteboard:zPasteBoard 
             source:self 
          slideBack:YES];
	</Abstract>
			<DeclaredIn>AZBoxGrid.m</DeclaredIn>			
			
			<Declaration>- (void)mouseDragged:(NSEvent *)event</Declaration>
			
			
			<Anchor>//api/name/mouseDragged:</Anchor>
